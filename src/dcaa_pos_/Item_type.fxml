<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dcaa_pos_.Item_typeController">
    <children>
        <Label layoutX="117.0" layoutY="81.0" text="Item Name">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <TextField fx:id="Item_name" layoutX="205.0" layoutY="76.0" prefHeight="31.0" prefWidth="299.0">
            <font>
                <Font size="15.0" />
            </font>
        </TextField>
        <Label layoutX="117.0" layoutY="112.0" text="Descrpition">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <TextField fx:id="Item_description" layoutX="205.0" layoutY="107.0" prefHeight="77.0" prefWidth="299.0">
            <font>
                <Font size="15.0" />
            </font>
        </TextField>
        <Label layoutX="35.0" layoutY="24.0" text="New Item Type">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <ButtonBar layoutX="17.0" layoutY="350.0" prefHeight="40.0" prefWidth="580.0">
            <buttons>
                <Button fx:id="Save_Button" mnemonicParsing="false" onAction="#Save_new_item" text="Add new Item Type">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Button>
                <Button fx:id="Close" mnemonicParsing="false" onAction="#Close" text="Close">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Button>
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
